{
  "manifest_version" : 2,
  "name": "FastOpen",
  "description": "Extension test",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": ["app.js"],
    "persistent": false
  },
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}